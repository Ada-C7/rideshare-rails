<section>
  <%= form_for @trip do |f| %>
    <%= f.label :driver_id %>
    <%= f.text_field :driver_id %>
    <%= f.label :passenger_id %>
    <%= f.text_field :passenger_id %>
    <%= f.label :date %>
    <%= f.text_field :date %>
    <%= f.label :price %>
    <%= f.text_field :price %>
    <%= f.label :rating %>
    <%= f.select :rating, [1, 2, 3, 4, 5]%>
    <%= f.submit value: "#{submit_text} trip" %>
  <% end %>
</section>
