<h1>Add a Trip</h1>

<section id = "_form_section">
  <%= form_for @trip , :url => { :action => "create" } do |f| %>


  <tr>
    <td><%= f.label :date, class: "label" %>
    <%= f.text_field :date, class: "field"%> </td>
  </tr>



    <div id = "_form_section_submit">
      <%= f.submit value: "SAVE"%>
    </div>

  <% end %>
</section>
