<ul class="errors">
  <% @rider.errors.each do |field, msg| %>
  <li>
    <%= field %> <%= msg %>
  </li>
    <% end %>

</ul>


<%= form_for @rider do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :phone_num, "Phone Number" %>
<%= f.text_field :phone_num %>

<%= f.submit "Save" %>

<% end %>
