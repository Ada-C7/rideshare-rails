<ul>
  <% @passenger.errors.each do |column, msg| %>
    <li>
      <%= column.capitalize %> <%= msg %>
    </li>
  <% end %>
</ul>


<ul class="form">
  <%= form_for @passenger, method: form_method do |f| %>
  <li><%= f.label :name %> <%= f.text_field :name %></li>
  <li><%= f.label nil, "Phone Number" %> <%= f.text_field :phone_num %></li>
  <li><%= f.submit nil %></li>
  <% end %>
</ul>
