
<%= form_for @passenger do | f | %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :phone_num %>
  <%= f.text_field :phone_num %>

  <%= f.submit "Submit", :class => 'button' %>

  <% end %>

  <ul>
    <% @passenger.errors.each do | field, msg | %>
    <li>
      <%= field %> <%= msg %>
    </li>
    <% end %>
  </ul>
